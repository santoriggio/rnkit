{
  "name": "expo-bare-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scheme": "expo-bare-app",
  "scripts": {
    "start": "npx expo start",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
